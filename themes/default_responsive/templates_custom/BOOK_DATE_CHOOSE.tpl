{+START,INCLUDE,FORM_SCREEN_INPUT_DATE}
	NAME={NAME}
	TYPE=date
	REQUIRED=1
	UNLIMITED=0
	DAY={CURRENT_DAY}
	MONTH={CURRENT_MONTH}
	YEAR={CURRENT_YEAR}
	MIN_DATE_DAY={MIN_DATE_DAY}
	MIN_DATE_MONTH={MIN_DATE_MONTH}
	MIN_DATE_YEAR={MIN_DATE_YEAR}
	MAX_DATE_DAY={MAX_DATE_DAY}
	MAX_DATE_MONTH={MAX_DATE_MONTH}
	MAX_DATE_YEAR={MAX_DATE_YEAR}
{+END}
